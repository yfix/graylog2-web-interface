    "gulp-rename": "*",
    "gulp-rev": "*",
    "gulp-rimraf": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "jest-cli": "*",
    "jquery": "*",
    "jshint-stylish": "*",
    "react-tools": "*",
    "reactify": "*",
    "rimraf": "*",
    "run-sequence": "*",
